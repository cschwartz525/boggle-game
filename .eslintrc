{
    "extends": "airbnb",
    "env": {
        "browser": true
    },
    "rules": {
        "comma-dangle": ["error", "never"],
        "eol-last": ["error", "never"],
        "indent": ["error", 4],
        "no-plusplus": 0,
        "react/jsx-filename-extension": 0,
        "react/jsx-indent": ["error", 4]
    }
}